        this.cantidadIngreso = 0;
        this.idsReingreso = [{
          id: "",
          cantidad: null
        }];
      } //Se obtiene inventario en orden alfabetico, y se imprime en la tabla//


      _createClass(ReingresoComponent, [{
        key: "ngOnInit",
        value: function ngOnInit() {
