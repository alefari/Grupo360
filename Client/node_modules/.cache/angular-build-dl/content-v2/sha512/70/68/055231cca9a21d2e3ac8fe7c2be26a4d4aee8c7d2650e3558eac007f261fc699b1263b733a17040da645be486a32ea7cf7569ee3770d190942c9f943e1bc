            return item.id == _this9.idItemElegido;
          });
        }
      }, {
        key: "borrarForm",
        value: function borrarForm() {
          this.form.reset();
        }
      }, {
        key: "agregarItem",
        value: function agregarItem() {
          this.idsReingreso.push({
            id: "",
            cantidad: null
          });
        }
      }, {
        key: "restarItem",
        value: function restarItem() {
          this.idsReingreso.pop();
        }
      }]);

